Algoritmo Divisas
	definir cantidad, mitad, euro, dolar como real;
	escribir "Ingresa la cantidad de pesos";
	Leer cantidad;
	mitad<-cantidad/2;
	dolar<-mitad/17.85;
	euro<-mitad/18.90;
	Escribir "La cantidad de dolares es $", dolar;
	escribir "La cantidad de euros es ?", euro;
	
FinAlgoritmo
