Algoritmo EcuacionCuadratica
	Definir a,b,c como real;
	definir x1,x2 como real;
	escribir "Porfavor, ingrese valor a";
	escribir "Porfavor, ingrese valor b";
	escribir "Porfavor, ingrese valor c";
	leer a, b, c;
	x1<-(-b+raiz(b*b-4*a*c))/(2*a);
	x2<-(-b-raiz(b*b-4*a*c))/(2*a);
	Escribir "valor x1= ", x1;
	escribir "valor x2= ", x2;
	
FinAlgoritmo
